{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "vueIndentScriptAndStyle": false,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
    "pluginSearchDirs": ["./frontend/node_modules"]
}
